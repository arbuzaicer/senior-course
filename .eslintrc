{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": ["error", "double"],
    "object-curly-spacing": ["error", "always"],
    "max-len": [
      "error",
      {
        "code": 200
      }
    ]
  },
  "extends": ["eslint:recommended", "google"]
}
